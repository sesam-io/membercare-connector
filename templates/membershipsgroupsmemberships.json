[
  {
    "_id": "{{@ system @}}-{{@ datatype @}}-all",
    "add_namespaces": false,
    "namespaced_identifiers": false,
    "source": {
      "dataset": "{{@ system @}}-membershipsgroups-collect",
      "type": "dataset"
    },
    "transform": [
      {
        "rules": {
          "default": [
            [
              "comment",
              "filter",
              [
                "eq",
                "_S.id",
                8
              ]
            ],
            [
              "copy",
              "*"
            ],
            [
              "add",
              "properties",
              [
                "dict",
                "id",
                "_S.id"
              ]
            ]
          ]
        },
        "type": "dtl"
      },
      {
        "id_expression": "{{ id }}",
        "operation": "{{@ datatype @}}-list",
        "replace_entity": true,
        "side_effects": false,
        "system": "{{@ system @}}",
        "type": "rest"
      }
    ],
    "type": "pipe"
  },
  {
    "_id": "{{@ system @}}-{{@ datatype @}}-collect",
    "source": {
      "dataset": "{{@ system @}}-{{@ datatype @}}-all",
      "type": "dataset"
    },
    "type": "pipe"
  }
]